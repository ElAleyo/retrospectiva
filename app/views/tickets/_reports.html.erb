<dl>
  <dt><%=_ 'Reports' %>:</dt>    

  <% @reports.each do |report| -%>
    <% next if report.user_specific? && User.current.public? -%>
    <dd><%= link_to_unless report == @report, h(report.name), project_tickets_path(Project.current, :report => report, :by => params[:by]) %></dd>
  <% end -%>
</dl>
